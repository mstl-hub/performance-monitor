{
    "version": 3,
    "configurePresets": [
        {
            "name": "TAF_default",
            "hidden": true,
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build/${presetName}",
            "toolchainFile": "${sourceDir}/cmake/gcc-arm-none-eabi_TAF.cmake",
            "cacheVariables": {
                "ADDED_DEF": "-DPIGSTORE_PBLOCK",
                "CMAKE_EXPORT_COMPILE_COMMANDS": true
            }
        },
        {
            "name": "TAF_Debug",
            "inherits": "TAF_default",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            },
            "hidden": true
        },
        {
            "name": "TAF_Release",
            "inherits": "TAF_default",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "ADDED_DEF": "-DUSE_WDT"
            },
            "hidden": true
        },
        {
            "name": "TAF_Debug peripheral-block",
            "inherits": "TAF_Debug",
            "cacheVariables": {
                "CMAKE_EXPORT_COMPILE_COMMANDS": true
            }
        },
        {
            "name": "TAF_Release peripheral-block",
            "inherits": "TAF_Release",
            "cacheVariables": {}
        }
    ],
    "buildPresets": [
        {
            "name": "TAF_Debug peripheral-block",
            "configurePreset": "TAF_Debug peripheral-block"
        },
        {
            "name": "TAF_Release peripheral-block",
            "configurePreset": "TAF_Release peripheral-block"
        }
    ]
}